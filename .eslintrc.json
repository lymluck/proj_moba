{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "no-continue": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "no-mixed-operators": "off",
    "guard-for-in": "off",
    "no-restricted-properties": "off",
    "no-restricted-syntax": "off",
    "no-param-reassign": "off"
  }
}